{
  "scripts": {
    "dev": "concurrently --kill-others --names \"tailwindcss,next dev\" \"pnpm run tailwindcss-dev\" \"pnpm run next-dev\"",
    "tailwindcss-dev": "npx tailwindcss -i input.css -o output.css --watch",
    "next-dev": "next dev",
    "build": "npx tailwindcss -i input.css -o output.css && next build"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.3",
    "@nextui-org/react": "^2.3.1",
    "@types/mdx": "^2.0.13",
    "framer-motion": "^11.1.1",
    "next": "^14.0.2",
    "nextra": "2.13.2",
    "nextra-theme-docs": "2.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-facebook": "^9.0.12",
    "react-icons": "^5.0.1",
    "remark-gfm": "^4.0.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^4.9.3"
  }
}